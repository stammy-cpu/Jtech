{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.0",
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
